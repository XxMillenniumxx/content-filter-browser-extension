{
  "manifest_version": 3,
  "name": "Web Content Filter (WORKSPACE)",
  "version": "1.1.0.4",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
      "page": "options.html"
   },
  "content_scripts": [
	{
      "matches": ["<all_urls>"],
	  "css": ["content_start.css"],
      "run_at": "document_start"
    },
	{
      "matches": ["<all_urls>"],
	  "all_frames": true,
      "js": ["content_start.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["<all_urls>"],
      "js": ["content_end.js"],
      "run_at": "document_idle"
    }
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}